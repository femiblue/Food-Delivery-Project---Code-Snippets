                <div class="alert alert-success" style="background-color: #ffffff;"> 
                <i class="fa fa-check-square"></i>
                    {% for msg in app.session.flashBag.get('success') %}
                        {{ msg }}
                    {% endfor %}
                </div>
            {% endif %}
            <div class="col-sm-6" style="border: 0px solid; border-right:1px dashed #DFA71E; margin-bottom: 20px; margin-top: 20px; ">
   
            <form class="well" action="{{ path('front_office_user_updateaddress') }}" method="post" {{ form_enctype(form) }} style="background:transparent; border:0px; color:#ffffff;">
            {{ form_widget(form) }}
             
                <div style="border: 0px solid; margin-top: 10px; position: relative;">
                    <button type="submit" class="btn btn-success" style="background-color: #DFA71E; border: 0px;">Update</button>
