{% include 'SuFrontOfficeBundle:Landing:header_top.html.twig' %}
<body>
{% block body %}
{% include 'SuFrontOfficeBundle:Landing:header.html.twig' %}


<section id="form"><!--form-->
		<div class="container">
			<div class="row">
               <div class="features_items">
				<div class="col-sm-4 col-sm-offset-4">
                    <h2 class="title text-center">Sign Up</h2>
					<div class="login-form"><!--login form-->
						
                       {% block fos_user_content %}
                      
                        <form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">
                            {{ form_widget(form) }}
                            <div>
                                
                                <button type="submit" id="_submit" name="_submit" class="btn btn-success" style="border-radius: 5px;"><i class="fa fa-plus" style="color: black;"></i>&nbsp;{{ 'registration.submit'|trans({}, 'FOSUserBundle') }}</button>
                            </div>
                        </form>
                        {% endblock fos_user_content %}
                          </div><!--/login form--><br /><br />
				</div>
			  </div>	
			</div>
		</div>
	</section><!--/form-->
	

{% include 'SuFrontOfficeBundle:Landing:footer.html.twig' %}

{% endblock %}


{% block javascripts %}
    <script src="{{ asset('js/jquery.js') }}"></script>
    <script src="{{ asset('js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('js/jquery.scrollUp.min.js') }}"></script>
    <script src="{{ asset('js/price-range.js') }}"></script>
    <script src="{{ asset('js/jquery.prettyPhoto.js') }}"></script>
    <script src="{{ asset('js/main.js') }}"></script>
{% endblock %}

</body>
</html>